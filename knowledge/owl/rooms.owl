    </owl:ObjectProperty>


    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#isRoomFunctionRoleOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#isRoomFunctionRoleOf">
