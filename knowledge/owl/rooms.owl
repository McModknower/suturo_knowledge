    </owl:class>


    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#RoomLocation -->

    <owl:Class rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#RoomLocation">
