        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#isSurfaceOf">
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Room">
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>


