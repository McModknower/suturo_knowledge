        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#RoomFunction"/>
        <rdfs:comment>A relation between a room and a its function</rdfs:comment>
    </owl:ObjectProperty>


    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DoorHandle -->

    <owl:Class rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DoorHandle">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#DesignedHandle"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DoorKnob"/>
        <rdfs:comment>Represents the handle of a door used to open or close the door</rdfs:comment>
    </owl:Class>


    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DoorKnob -->

    <owl:Class rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DoorKnob">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#DesignedHandle"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DoorHandle"/>
        <rdfs:comment>Represents a knob of a door used to open or close a door. In comparison to a door handle which is mostly found at room doors door knoby are mostly part of furniture doors.</rdfs:comment>
    </owl:Class>


    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#RoomFunction -->

    <owl:Class rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#RoomFunction">
        <rdfs:subClassOf>
            <rdfs:restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#isRoomFunctionRoleOf"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Room"/>
            </rdfs:restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Represents the function a rrom has in a household</rdfs:comment>
    </owl:Class>


    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#Kitchen -->

    <owl:Class rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#Kitchen">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#RoomFunction"/>
        <rdfs:comment>Represents a kitchen, which is a specific kind of room in an household, where people are cooking</rdfs:comment>
    </owl:Class>



    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DiningRoom -->

    <owl:Class rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DiningRoom">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#RoomFunction"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#Kitchen"/>
        <rdfs:comment>Represents a kitchen, which is a specific kind of room in an household, where people are eating</rdfs:comment>
    </owl:Class>


    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#LivingRoom -->

    <owl:Class rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#LivingRoom">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#RoomFunction"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#Kitchen"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DiningRoom"/>
        <rdfs:comment>Represents a living room, which is a specific kind of room in an household, where people are living</rdfs:comment>
    </owl:Class>


    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#SleepingRoom -->

    <owl:Class rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#SleepingRoom">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#RoomFunction"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#Kitchen"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DiningRoom"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#LivingRoom"/>
        <rdfs:comment>Represents a sleeping room in an household, which is a specific kind of room, where people are sleeping</rdfs:comment>
    </owl:Class>


    <!-- http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#UnknownRoom -->

    <owl:Class rdf:about="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#UnknownRoom">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#RoomFunction"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#Kitchen"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#DiningRoom"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#LivingRoom"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#SleepingRoom">
        <rdfs:comment>Represents a room with an unknown function</rdfs:comment>
    </owl:Class>


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Room -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Room">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalPlace"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#hasRoomFunctionRole">
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#RoomFunction">
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#hasSurface">
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/suturo/ontologies/2021/0/rooms#RoomSurface">
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>


    <!-- http://www.ease-crc.org/ont/SOMA.owl#RoomSurface -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#RoomSurface">
